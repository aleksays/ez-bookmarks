{
  "manifest_version": 3,
  "version": "0.0.1",
  "short_name": "EzBookmarks",
  "name": "Easy bookmarks",
  "description": "Extension to easy use bookmarks",
  "author": "Alex Levicky",
  "permissions": ["storage", "tabs", "activeTab", "bookmarks", "favicon"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": {
      "16": "logo192.png",
      "24": "logo192.png",
      "32": "logo192.png"
    },
    "default_title": "Easy Bookmarks",
    "default_popup": "popup.html"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  }
}
